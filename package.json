{
	"name": "@uwu-labs/on-chain-owners",
	"version": "1.0.0",
	"author": "Quantumlyy <yo@quantumly.dev>",
	"contributes": {
		"name": "Sporidium.eth", 
		"email": "spori.labs@gmail.com"
	},
	"license": "UNLICENSED",
	"private": true,
	"type": "module",
	"main": "dist/cli.js",
	"bin": {
		"owners": "./dist/cli.js"
	},
	"imports": {
		"#prompts": "./dist/prompts/index.js",
		"#prompts/*": "./dist/prompts/*.js",
		"#functions": "./dist/functions/index.js",
		"#functions/*": "./dist/functions/*.js",
		"#commands/*": "./dist/commands/*.js",
		"#constants": "./dist/constants.js"
	},
	"sideEffects": "false",
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"!dist/**/*.js.map",
		"!dist/**/*.d.ts",
		"!dist/**/*.d.ts.map",
		"templates"
	],
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts",
		"update": "yarn up \"*\" -i",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"dev": "yarn build && node ./dist/cli.js",
		"sversion": "standard-version -a",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"chalk": "^5.0.0",
		"commander": "^9.0.0",
		"dotenv": "^16.0.1",
		"ethers": "^5.5.4",
		"graphql": "^16.3.0",
		"graphql-request": "^4.0.0",
		"ora": "^6.0.1",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.2.0",
		"@sapphire/prettier-config": "^1.3.0",
		"@sapphire/ts-config": "^3.3.1",
		"@subgraphs/eip721-matic": "^1.0.1",
		"@types/node": "^17.0.15",
		"@types/prompts": "^2.0.14",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.3.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.5"
	},
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"prettier": "@sapphire/prettier-config",
	"repository": "https://github.com/uwu-labs/on-chain-owners.git"
}
