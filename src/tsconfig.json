{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"rootDir": "./",
		"outDir": "../dist",
		"composite": true,
		"preserveConstEnums": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"lib": [
            "es5", "es6", "dom", "dom.iterable"
        ]
	},
	"include": ["."]
}
